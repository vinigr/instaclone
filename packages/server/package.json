{
  "name": "@instaclone/server",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "npm run clear && babel src --extensions \".es6,.js,.es,.jsx,.mjs,.ts\" --ignore *.spec.js --out-dir dist --copy-files",
    "clear": "rm -rf ./dist",
    "dev": "webpack --watch --progress --config webpack.config.js",
    "start": "npm run build && node dist/index.js",
    "test": "jest",
    "update-schema": "babel-node --extensions \".es6,.js,.es,.jsx,.mjs,.ts\" ./scripts/updateSchema.ts"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "bcryptjs": "^2.4.3",
    "dotenv-safe": "^8.2.0",
    "graphql": "^16.5.0",
    "graphql-playground-middleware-koa": "^1.6.22",
    "graphql-relay": "^0.10.0",
    "jsonwebtoken": "^8.5.1",
    "kcors": "^2.2.2",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-graphql": "^0.12.0",
    "koa-router": "^11.0.1",
    "mongoose": "^6.4.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/node": "^7.18.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv-safe": "^8.1.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/kcors": "^2.2.6",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/koa-router": "^7.4.4",
    "babel-loader": "^8.2.5",
    "jest-fetch-mock": "^3.0.3",
    "mongodb-memory-server-global": "^8.7.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  }
}
